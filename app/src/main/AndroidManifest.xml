<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="hr.tomljanovic.matko.seminarskirad">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:name=".utils.RoomApp"
        android:allowBackup="true"
        android:icon="@mipmap/ic_hacker"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/ic_hacker_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.AppCompat.Light.NoActionBar"
        android:usesCleartextTraffic="true">
        <activity android:name=".settings.SettingsActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".view.savedArticles.SavedArticles"
            android:screenOrientation="portrait" />
        <activity android:name=".view.webActivity.WebViewActivity"
            android:screenOrientation="portrait"/>
        <activity android:name=".view.newsFeed.MainActivity"
            android:screenOrientation="portrait">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>